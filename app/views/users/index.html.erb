<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Is staff</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.is_staff %></td>
        <td><%= link_to 'change is_staff', show_user_path(id: user.id) %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<td><%= link_to 'Back', games_path %></td>

